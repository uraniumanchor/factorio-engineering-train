{
  "name": "factorio-engineering-train",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "immer": "^6.0.9",
    "pako": "^1.0.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "fix:prettier": "prettier --write \"src/**\"",
    "fix:lint": "yarn lint --fix",
    "fix:all": "yarn fix:prettier && yarn fix:lint",
    "lint": "eslint src --ext js --ext jsx --ext ts --ext tsx --quiet",
    "prettier": "prettier --check \"src/**\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/pako": "^1.0.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.5"
